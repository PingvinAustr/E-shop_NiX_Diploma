@model List<CatalogItem>

<h2>Selected Cars</h2>

@if (Model.Any())
{
    <table class="table">
        <thead>
            <tr>
                <th>Car Name</th>
                <th>Price</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var car in Model)
            {
                <tr>
                    <td>@car.CarName</td>
                    <td>@car.Price.ToString("C")</td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>No cars selected</p>
}
